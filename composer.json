{
    "name": "jpchateau/php-prototype-library",
    "type": "library",
    "license": "MIT",
    "description": "My PHP library",
    "keywords": ["php", "library"],
    "require": {
        "php": "^7.1",
        "ext-dom": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Foo\\Bar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Foo\\Bar\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}