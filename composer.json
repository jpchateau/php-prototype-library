{
    "name": "jpchateau/php-prototype-library",
    "type": "library",
    "description": "Quickly initiate the development of a new PHP library",
    "keywords": ["php", "library"],
    "require": {
        "php": "^7.3",
        "ext-dom": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MyCompany\\MyProject\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyCompany\\MyProject\\Tests\\": "tests"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    }
}